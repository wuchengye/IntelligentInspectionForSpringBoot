<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bda.bdaqm.risk.mapper.DubiousMapper">
	
	
	<select id ="getDubiousData" resultType = "com.bda.bdaqm.risk.model.NotDubious">
		select * from bdaqm_not_dubious 
		where 1=1
		<if test="fileName != null and fileName != ''">
			and file_name like CONCAT('%',#{fileName},'%')
		</if>
		<if test="recordStartDate != null and recordStartDate != ''">
			and record_date <![CDATA[>=]]> #{recordStartDate}
		</if>
		<if test="recordEndDate != null and recordEndDate != ''">
			and record_date <![CDATA[<=]]> #{recordEndDate}
		</if>
	</select>
	
	<select id = "getDubiousById" resultType="com.bda.bdaqm.risk.model.NotDubious">
		select *  from bdaqm_not_dubious 
		where session_id=#{sessionId}
	
	</select>
	
	
	<select id = "getSessionDetail" resultType="com.bda.bdaqm.risk.model.SessionDetail">
		select *  from bdaqm_not_dubious_session_detail
		where stl_session_id = #{sessionId}
	</select>
	
	
	
</mapper>